<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Blog PaperSmile</title>
    <link href="http://blog.papersmile.pl/atom.xml" rel="self" />
    <link href="http://blog.papersmile.pl/" />
    <id>http://blog.papersmile.pl/</id>
    <logo>http://papersmile.pl/img/logo.jpg</logo>
    <icon>http://papersmile.pl/favicon.ico</icon>
    <updated>{% for post in site.posts %}{% if forloop.first %}{{ post.rssdate }}{% endif %}{% endfor %}</updated>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://{{ site.url }}{{ post.url | remove: 'index.html' | replace: '.html', '' }}" />
        <id>http://{{ site.url }}{{ post.url | remove: 'index.html' | replace: '.html', '' }}</id>
        <logo>http://blog.papersmile.pl/img/p/{{ post.img }}</logo>
        <updated>{{ post.rssdate }}</updated>
        <summary>{{ post.teaser }}</summary>
        <author>
            <name>PaperSmile</name>
        </author>
    </entry>
    {% endfor %}
</feed>