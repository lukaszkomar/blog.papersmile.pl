<?xml version="1.0" encoding="utf-8"?>
<feed xmlns="http://www.w3.org/2005/Atom">

    <title>Blog PaperSmile</title>
    <link href="http://blog.papersmile.pl/atom.xml" rel="self" />
    <link href="http://blog.papersmile.pl" />
    <id>blogpapersmile</id>
    <updated>{% for post in site.posts %}{% if forloop.last %}{{ post.date }}{% endif %}{% endfor %}</updated>

    {% for post in site.posts %}
    <entry>
        <title>{{ post.title }}</title>
        <link href="http://{{ site.url }}{{ post.url | remove: 'index.html' | replace: '.html', '' }}" />
        <id>urn:uuid:{{ post.id }}</id>
        <updated>{{ post.date }}</updated>
        <summary>{{ post.teaser }}</summary>
        <author>
            <name>PaperSmile</name>
        </author>
    </entry>
    {% endfor %}
</feed>